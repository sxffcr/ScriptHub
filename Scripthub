local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local ScriptHub = library:CreateWindow("ScriptHub") -- Creates the window

local Games = ScriptHub:CreateFolder("Games") -- Creates the folder(U will put here your buttons,etc)

Games:Label("ScriptHub ni Jack",{
	TextSize = 25; -- Self Explaining
	TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
	BgColor = Color3.fromRGB(69,69,69); -- Self Explaining

}) 

local NinjaLegends2 = Games:CreateFolder("Ninja Legends 2")
Games:Button("Ninja Legends 2",function()
	loadstring(game:HttpGet("https://sinnyno1.github.io/King-Hack/Script/Ninja-Legend-2.lua",true))();
end)

local RoWizard = Games:CreateFolder("Ro-Wizard")
Games:Button("Ro-Wizard",function()
	loadstring(game:HttpGet("https://pastebin.com/raw/cABME65h", true))();
end)

local JailBreak = Games:CreateFolder("JailBreak")
Games:Button("JailBreak",function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/piglex9/jailbreak-ice-tray/main/Main.lua',true))()
end)

local Specter = Games:CreateFolder("Specter")
Games:Button("Lobby GUI",function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/venosu/specter-hax/main/misc.lua"))()
end)
Games:Button("In-Game GUI",function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/venosu/specter-hax/main/game.lua"))()
end)

local Isle = Games:CreateFolder("Isle")
Games:Button("Isle",function()
	loadstring(game:HttpGetAsync("https://paste.ee/r/f2sGK/0"))()
end)

Games:DestroyGui()
